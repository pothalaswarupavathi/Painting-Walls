import java.util.*;
import java.lang.*;
import java.io.*;

class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
		Scanner in=new Scanner(System.in);
		int t=in.nextInt();
		for(int i=0;i<t;i++){
		    int x=in.nextInt();
		    int y=in.nextInt();
		    int z=in.nextInt();
		    int m=x*y;
		    if(m<=(z/2)){
		        System.out.println((z/2)/m);
		    }
		    else{
		        System.out.println("0");
		    }
		}
		// your code goes here

	}
}
